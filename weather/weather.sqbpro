CREATE VIEW weatherss AS 
select a.City,b.tempture jan,c.tempture feb,d.tempture march,e.tempture apr,f.tempture june,g.tempture july,h.tempture Aug,i.tempture sep,j.tempture oct,k.tempture nov,l.tempture dec
from Weather a
INNER JOIN Weather b on a.City=b.City and b.month=1 and a.month=1
