INNER JOIN Weather d on a.City=d.City and d.month=3
INNER JOIN Weather e on a.City=e.City and e.month=4
INNER JOIN Weather f on a.City=f.City and f.month=5
INNER JOIN Weather g on a.City=g.City and g.month=6
INNER JOIN Weather h on a.City=h.City and h.month=8
INNER JOIN Weather i on a.City=i.City and i.month=9
INNER JOIN Weather j on a.City=j.City and j.month=10
INNER JOIN Weather k on a.City=k.City and k.month=11
INNER JOIN Weather l on a.City=l.City and l.month=12</sql><current_tab id="0"/></tab_sql></sqlb_project>
